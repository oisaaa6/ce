-- UI设置标签页
local SettingsTab = window:Tab("UI设置",'6035145364')

local settingsSection = SettingsTab:section("UI设置",true)

settingsSection:Button("摧毁UI",function()
    game:GetService("CoreGui")["frosty"]:Destroy()
end)

settingsSection:Toggle("彩虹UI", "", false, function(state)
    if state then
        game:GetService("CoreGui")["frosty"].Main.Style = "DropShadow"
    else
        game:GetService("CoreGui")["frosty"].Main.Style = "Custom"
    end
end)

settingsSection:Toggle("玩家加入游戏提示", "", false, function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/boyscp/scriscriptsc/main/bbn.lua"))()
    end
end)
