-- 初始化UI
Library:UpdateUIStyle()

-- 添加缺失的函数定义
function SaveConfig(name)
    local configData = {
        uiSettings = Library.uiSettings,
        uiPosition = Library.uiPosition,
        configName = name,
        saveTime = os.time()
    }
    
    if writefile then
        writefile("BlackHoleUI_" .. name .. ".json", game:GetService("HttpService"):JSONEncode(configData))
    end
    
    return true, "配置保存成功: " .. name
end

function LoadConfig(name)
    if readfile and isfile then
        local filename = "BlackHoleUI_" .. name .. ".json"
        if isfile(filename) then
            local configData = game:GetService("HttpService"):JSONDecode(readfile(filename))
            for key, value in pairs(configData.uiSettings) do
                if Library.uiSettings[key] ~= nil then
                    Library.uiSettings[key] = value
                end
            end
            Library.uiPosition = configData.uiPosition
            Library:UpdateUIStyle()
            return true, "配置加载成功: " .. name
        end
    end
    return false, "配置不存在: " .. name
end

function DeleteConfig(name)
    if delfile and isfile then
        local filename = "BlackHoleUI_" .. name .. ".json"
        if isfile(filename) then
            delfile(filename)
            return true, "配置删除成功: " .. name
        end
    end
    return false, "配置不存在: " .. name
end

-- 自动保存位置
game:GetService("RunService").Heartbeat:Connect(function()
    Library.uiPosition = Library.uiElements.Main.Position
end)

-- 启动时加载最新配置
task.spawn(function()
    task.wait(2)
    local success, message = LoadConfig("默认配置")
end)
