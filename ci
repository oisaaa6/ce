local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/oisaaa6/UI/refs/heads/main/UI"))()
local Window = Library:new("BP脚本中心V1")

-- 配置UI标签页
do
    local settingssTab = Window:Tab("配置UI", "92237541628279")
    local configSection = settingssTab:section("配置管理", true)
    
    -- 配置管理功能
    function configSection:ConfigManager()
        local configName = "默认配置"
        local statusLabel = self:Label("当前配置: " .. configName)

        self:Button("重置所有UI", function()
            statusLabel.Text = "UI样式已重置为默认值"
        end)

        self:Button("保存UI位置", function()
            statusLabel.Text = "UI位置已保存"
        end)

        self:Button("恢复UI位置", function()
            statusLabel.Text = "UI位置已恢复"
        end)

        local configTextbox = self:Textbox("配置名称", "config_name", configName, function(value)
            configName = value
        end)

        self:Button("保存配置", function()
            statusLabel.Text = "配置已保存: " .. configName
        end)

        self:Button("加载配置", function()
            statusLabel.Text = "配置已加载: " .. configName
        end)

        self:Button("删除配置", function()
            statusLabel.Text = "配置已删除: " .. configName
        end)

        self:Toggle("启用自动保存", "auto_save", false, function(state)
            if state then
                statusLabel.Text = "自动保存已启用"
            else
                statusLabel.Text = "自动保存已禁用"
            end
        end)
    end
    
    -- UI样式设置功能
    function configSection:UIStyleSettings()
        local statusLabel = self:Label("UI样式设置")
        
        self:Dropdown("选择主题", "ui_theme", {"Default", "Dark", "Light", "Cyberpunk"}, function(selected)
            statusLabel.Text = "已应用主题: " .. selected
        end)
        
        self:Colorpicker("主色调", Color3.new(1, 1, 1), function(color)
            statusLabel.Text = "主色调已更新"
        end)
        
        self:Slider("透明度", "UI透明度", 0, 100, 100, function(value)
            statusLabel.Text = "透明度已设置: " .. value .. "%"
        end)
    end
    
    configSection:ConfigManager()
    
    local styleSection = settingssTab:section("UI样式设置", true)
    styleSection:UIStyleSettings()
end

local player = game.Players.LocalPlayer

-- 通用功能标签页
local MainTab = Window:Tab("通用功能", '92237541628279')

local mainSection1 = MainTab:section("角色属性设置", true)

mainSection1:Slider("移动速度", "WalkSpeed", 16, 200, 16, function(Value)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = Value
    end
end)

mainSection1:Slider("跳跃高度", "JumpPower", 50, 200, 50, function(Value)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.JumpPower = Value
    end
end)

mainSection1:Button("重置角色属性", function()
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = 16
        player.Character.Humanoid.JumpPower = 50
    end
end)

mainSection1:Button("设置重力", function()
    local value = game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("SetGravity").Value
    game.Workspace.Gravity = tonumber(value) or 196.2
end)

local mainSection2 = MainTab:section("视觉功能", true)

mainSection2:Toggle("夜视模式", "NightVision", false, function(state)
    if state then
        game.Lighting.Ambient = Color3.new(1, 1, 1)
        game.Lighting.Brightness = 2
    else
        game.Lighting.Ambient = Color3.new(0, 0, 0)
        game.Lighting.Brightness = 1
    end
end)

local Noclip = false
local Stepped

mainSection2:Toggle("穿墙模式", "Noclip", false, function(state)
    if state then
        Noclip = true
        Stepped = game.RunService.Stepped:Connect(function()
            if Noclip and player.Character then
                for _, part in pairs(player.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            else
                if Stepped then
                    Stepped:Disconnect()
                end
            end
        end)
    else
        Noclip = false
        if Stepped then
            Stepped:Disconnect()
        end
    end
end)

mainSection2:Toggle("全屏亮光", "FullBright", false, function(state)
    if state then
        game.Lighting.GlobalShadows = false
        game.Lighting.Brightness = 2
    else
        game.Lighting.GlobalShadows = true
        game.Lighting.Brightness = 1
    end
end)

