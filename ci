mainSection3:Button("点击传送工具", function()
    local mouse = player:GetMouse() 
    local tool = Instance.new("Tool") 
    tool.RequiresHandle = false 
    tool.Name = "[FE] TELEPORT TOOL" 
    tool.Activated:connect(function() 
        local pos = mouse.Hit + Vector3.new(0, 2.5, 0) 
        pos = CFrame.new(pos.X, pos.Y, pos.Z) 
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = pos 
        end
    end) 
    tool.Parent = player.Backpack
end)

mainSection3:Button("无敌模式", function()
    local lp = game:GetService("Players").LocalPlayer
    if lp.Character and lp.Character:FindFirstChild("Head") then
        local char = lp.Character
        char.Archivable = true
        local new = char:Clone()
        new.Parent = workspace
        lp.Character = new
        wait(2)
        local oldhum = char:FindFirstChildWhichIsA("Humanoid")
        local newhum = oldhum:Clone()
        newhum.Parent = char
        newhum.RequiresNeck = false
        oldhum.Parent = nil
        wait(2)
        lp.Character = char
        new:Destroy()
        wait(1)
        newhum:GetPropertyChangedSignal("Health"):Connect(function()
            if newhum.Health <= 0 then
                oldhum.Parent = lp.Character
                wait(1)
                oldhum:Destroy()
            end
        end)
        workspace.CurrentCamera.CameraSubject = char
        if char:FindFirstChild("Animate") then
            char.Animate.Disabled = true
            wait(.1)
            char.Animate.Disabled = false
        end
        if char:FindFirstChild("Head") then
            char.Head:Destroy()
        end
    end
end)

mainSection3:Button("高级透视", function()
    loadstring(game:GetObjects("rbxassetid://10092697033")[1].Source)()
end)

mainSection3:Button("紫砂功能", function()
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.Health = 0
    end
end)

