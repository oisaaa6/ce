local RainbowLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/YJY2022hh666/yjy/main/rainbow.ui.main.lua?token=GHSAT0AAAAAACESL5MYP6BL6PJ45CVEZRQEZFYXXUA",true))()
local window = library:new("Blue paint 2.0")

local player = game.Players.LocalPlayer

-- 通用功能标签页
local MainTab = window:Tab("通用功能",'6035145364')

local mainSection1 = MainTab:section("角色属性设置",true)

mainSection1:Slider("移动速度", "调整角色移动速度 (16-200)", 16, 200, 16, function(Value)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = Value
    end
end)

mainSection1:Slider("跳跃高度", "调整角色跳跃高度 (50-200)", 50, 200, 50, function(Value)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.JumpPower = Value
    end
end)

mainSection1:Button("设置跳跃高度", function()
    local value = game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("SetJumpPower").Value
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.JumpPower = tonumber(value) or 50
    end
end)

mainSection1:Button("设置移动速度", function()
    local value = game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("SetWalkSpeed").Value
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = tonumber(value) or 16
    end
end)

mainSection1:Button("设置重力", function()
    local value = game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("SetGravity").Value
    game.Workspace.Gravity = tonumber(value) or 196.2
end)

local mainSection2 = MainTab:section("视觉功能",true)

mainSection2:Toggle("夜视模式", "", false, function(state)
    if state then
        game.Lighting.Ambient = Color3.new(1, 1, 1)
        game.Lighting.Brightness = 2
    else
        game.Lighting.Ambient = Color3.new(0, 0, 0)
        game.Lighting.Brightness = 1
    end
end)

local Noclip = false
local Stepped

mainSection2:Toggle("穿墙模式", "", false, function(state)
    if state then
        Noclip = true
        Stepped = game.RunService.Stepped:Connect(function()
            if Noclip and player.Character then
                for _, part in pairs(player.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            else
                if Stepped then
                    Stepped:Disconnect()
                end
            end
        end)
    else
        Noclip = false
        if Stepped then
            Stepped:Disconnect()
        end
    end
end)

