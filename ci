-- UIè®¾ç½®æ ‡ç­¾é¡µ
local SettingsTab = Window:Tab("UIè®¾ç½®", '92237541628279')
local settingsSection = SettingsTab:section("UIè®¾ç½®", true)

settingsSection:Button("æ‘§æ¯UI", function()
    if game:GetService("CoreGui"):FindFirstChild("REN") then
        game:GetService("CoreGui").REN:Destroy()
    end
end)

settingsSection:Toggle("å½©è™¹UI", "RainbowUI", false, function(state)
    if state then
        spawn(function()
            while Library.flags.RainbowUI do
                for i = 0, 1, 0.01 do
                    if not Library.flags.RainbowUI then break end
                    local hue = i * 360
                    local color = Color3.fromHSV(hue/360, 1, 1)
                    Library.uiSettings.mainColor = color
                    Library:UpdateUIStyle()
                    task.wait(0.05)
                end
            end
        end)
    end
end)

settingsSection:Toggle("ç©å®¶åŠ å…¥æ¸¸æˆæç¤º", "PlayerJoinNotify", false, function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/boyscp/scriscriptsc/main/bbn.lua"))()
    end
end)

settingsSection:Slider("UIé€æ˜åº¦", "UITransparency", 0, 100, 100, function(value)
    Library.uiSettings.backgroundColorTransparency = (100 - value) / 100
    Library:UpdateUIStyle()
end)

settingsSection:Slider("UIå¤§å°", "UISize", 50, 150, 100, function(value)
    Library.uiSettings.uiSize = value / 100
    Library:UpdateUIStyle()
end)

settingsSection:Colorpicker("ä¸»è‰²è°ƒ", Color3.new(1, 1, 1), function(color)
    Library.uiSettings.mainColor = color
    Library:UpdateUIStyle()
end)

settingsSection:Colorpicker("èƒŒæ™¯è‰²", Color3.new(0, 0, 0), function(color)
    Library.uiSettings.backgroundColor = color
    Library:UpdateUIStyle()
end)

settingsSection:Colorpicker("æ–‡æœ¬é¢œè‰²", Color3.new(1, 1, 1), function(color)
    Library.uiSettings.textColor = color
    Library:UpdateUIStyle()
end)

settingsSection:Button("é‡ç½®æ‰€æœ‰è®¾ç½®", function()
    for key, value in pairs(Library.uiSettings) do
        if key == "mainColor" then Library.uiSettings[key] = Color3.new(0, 0, 0)
        elseif key == "backgroundColor" then Library.uiSettings[key] = Color3.new(0, 0, 0)
        elseif key == "textColor" then Library.uiSettings[key] = Color3.new(1, 1, 1)
        elseif key == "backgroundColorTransparency" then Library.uiSettings[key] = 0.5
        elseif key == "uiSize" then Library.uiSettings[key] = 1.0
        end
    end
    Library:UpdateUIStyle()
end)

settingsSection:Button("å¯¼å‡ºé…ç½®", function()
    local configData = game:GetService("HttpService"):JSONEncode(Library.uiSettings)
    setclipboard(configData)
end)

settingsSection:Button("å¯¼å…¥é…ç½®", function()
    local configData = game:GetService("HttpService"):JSONDecode(readfile("BlackHoleUI_config.json") or "{}")
    for key, value in pairs(configData) do
        if Library.uiSettings[key] ~= nil then
            Library.uiSettings[key] = value
        end
    end
    Library:UpdateUIStyle()
end)

