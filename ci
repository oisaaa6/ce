-- UIè®¾ç½®æ ‡ç­¾é¡µ
local SettingsTab = window:Tab("UIè®¾ç½®",'6035145364')

local settingsSection = SettingsTab:section("UIè®¾ç½®",true)

settingsSection:Button("æ‘§æ¯UI",function()
    game:GetService("CoreGui")["frosty"]:Destroy()
end)

settingsSection:Toggle("å½©è™¹UI", "", false, function(state)
    if state then
        game:GetService("CoreGui")["frosty"].Main.Style = "DropShadow"
    else
        game:GetService("CoreGui")["frosty"].Main.Style = "Custom"
    end
end)

settingsSection:Toggle("ç©å®¶åŠ å…¥æ¸¸æˆæç¤º", "", false, function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/boyscp/scriscriptsc/main/bbn.lua"))()
    end
end)
